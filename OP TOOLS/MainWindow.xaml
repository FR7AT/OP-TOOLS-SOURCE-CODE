<Window x:Class="OP_TOOLS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        Title="OP TOOLS"
        Height="522"
        Width="800"
        ResizeMode="CanMinimize" 
        Background="{DynamicResource MaterialDesignPaper}"
        WindowStartupLocation="CenterScreen" Icon="OP.png">
    
    <Grid>
        <DockPanel>

            <StackPanel Width="180" Background="#0a0b0f" VerticalAlignment="Stretch">
                
                <Grid HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10,20,10,20">
                    <TextBlock x:Name="AnimatedTextBlock" FontSize="25" FontWeight="Bold">
                        <TextBlock.Foreground>
                            <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                                <GradientStop Color="White" Offset="0" />
                                <GradientStop Color="#0088FF" Offset="0.5" x:Name="BlueGradientStop" />
                                <GradientStop Color="White" Offset="1" />
                            </LinearGradientBrush>
                        </TextBlock.Foreground>
                    </TextBlock>
                </Grid>

                <ListBox Background="#0a0b0f" BorderThickness="0" Padding="0" SelectionChanged="ListBox_SelectionChanged">
                    
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="ListBoxItem">
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ListBoxItem">
                                        <Border Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="1">
                                            <ContentPresenter HorizontalAlignment="Stretch"
                                                  VerticalAlignment="Center" />
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Background" Value="Transparent" />
                            <Setter Property="Foreground" Value="#ECF0F1" />
                            <Setter Property="Cursor" Value="Hand" />
                            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="#1F2933" />
                                    <Setter Property="Foreground" Value="#FFFFFF" />
                                    <Setter Property="BorderBrush" Value="#0088FF" />
                                </Trigger>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Background" Value="#1B1F24" />
                                    <Setter Property="Foreground" Value="#00C4FF" />
                                    <Setter Property="BorderBrush" Value="#00A1FF" />
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </ListBox.ItemContainerStyle>

                    <ListBoxItem Tag="HomePage" Height="81" Padding="1">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                            <materialDesign:PackIcon Kind="Home" Width="40" Height="40" Foreground="#ECF0F1" Margin="10"/>
                            <TextBlock Text="Home" VerticalAlignment="Center" Foreground="#ECF0F1" FontSize="18"/>
                        </StackPanel>
                    </ListBoxItem>

                    <ListBoxItem Tag="GameLoopPage" Height="81" Padding="1">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                            <materialDesign:PackIcon Kind="GamepadVariant" Width="40" Height="40" Foreground="#ECF0F1" Margin="10"/>
                            <TextBlock Text="GameLoop" VerticalAlignment="Center" Foreground="#ECF0F1" FontSize="18"/>
                        </StackPanel>
                    </ListBoxItem>

                    <ListBoxItem Tag="AndroidPage" Height="81" Padding="1">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                            <materialDesign:PackIcon Kind="Android" Width="40" Height="40" Foreground="#ECF0F1" Margin="10"/>
                            <TextBlock Text="Android" VerticalAlignment="Center" Foreground="#ECF0F1" FontSize="18"/>
                        </StackPanel>
                    </ListBoxItem>

                    <ListBoxItem Tag="SystemPage" Height="81" Padding="1">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                            <materialDesign:PackIcon Kind="Cogs" Width="40" Height="40" Foreground="#ECF0F1" Margin="10"/>
                            <TextBlock Text="System" VerticalAlignment="Center" Foreground="#ECF0F1" FontSize="18"/>
                        </StackPanel>
                    </ListBoxItem>

                    <ListBoxItem Tag="ProgramsPage" Height="81" Padding="1">
                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                            <materialDesign:PackIcon Kind="Application" Width="40" Height="40" Foreground="#ECF0F1" Margin="10"/>
                            <TextBlock Text="Programs" VerticalAlignment="Center" Foreground="#ECF0F1" FontSize="18"/>
                        </StackPanel>
                    </ListBoxItem>
                
                </ListBox>

                <StackPanel.Triggers>
                    
                    <EventTrigger RoutedEvent="Loaded">
                        <BeginStoryboard>
                            <Storyboard>
                                <StringAnimationUsingKeyFrames Storyboard.TargetName="AnimatedTextBlock" Storyboard.TargetProperty="Text">
                                    <DiscreteStringKeyFrame Value="" KeyTime="0:0:0" />
                                    <DiscreteStringKeyFrame Value="O" KeyTime="0:0:0.2" />
                                    <DiscreteStringKeyFrame Value="OP" KeyTime="0:0:0.4" />
                                    <DiscreteStringKeyFrame Value="OP " KeyTime="0:0:0.6" />
                                    <DiscreteStringKeyFrame Value="OP T" KeyTime="0:0:0.8" />
                                    <DiscreteStringKeyFrame Value="OP TO" KeyTime="0:0:1.0" />
                                    <DiscreteStringKeyFrame Value="OP TOO" KeyTime="0:0:1.2" />
                                    <DiscreteStringKeyFrame Value="OP TOOL" KeyTime="0:0:1.4" />
                                    <DiscreteStringKeyFrame Value="OP TOOLS" KeyTime="0:0:1.6" />
                                </StringAnimationUsingKeyFrames>

                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                
                </StackPanel.Triggers>
            
            </StackPanel>

            <Grid>
                
                <Grid x:Name="HomePage" Visibility="Visible" Background="#242424">
                    <Image Source="pack://application:,,,/Images/Home.png" Stretch="UniformToFill" />
                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="10,15,10,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock x:Name="VersionTextBlock" Grid.Column="0" FontSize="12" FontWeight="Bold" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Top" />

                        <TextBlock x:Name="DeveloperTextBlock" Grid.Column="1" FontSize="12" FontWeight="Bold" Foreground="White" HorizontalAlignment="Right" VerticalAlignment="Top" Text="Dev By : Farhat" />
                    </Grid>
                </Grid>

                <Grid x:Name="GameLoopPage" Visibility="Collapsed" Background="#242424">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <Button Grid.Row="0" Grid.Column="0" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="ينسخ الموارد إلى الشير 1 ويجب يكون المحاكي شغال" Click="PaksOut_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="ArrowLeftBoldCircle" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Paks Out" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <ComboBox Grid.Row="0" Grid.Column="1" Width="180" Height="50" Margin="10"
          FontSize="14" FontWeight="Bold" Foreground="White" Background="#31384E"
          HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
          ToolTip="اختر نسخة ببجي" SelectionChanged="ComboBox_SelectionChanged">
                        <ComboBox.Template>
                            <ControlTemplate TargetType="ComboBox">
                                <Grid>
                                    <Border x:Name="border" Background="#31384E" CornerRadius="8"
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <Grid>
                                            <ContentPresenter Content="{TemplateBinding SelectionBoxItem}"
                                          VerticalAlignment="Center"
                                          HorizontalAlignment="Left"
                                          Margin="10,0,10,0" />
                                            <ToggleButton x:Name="DropDownToggle" Background="Transparent" BorderThickness="0"
                                      IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                      HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Height="30" Cursor="Hand">
                                                <Grid>
                                                    <Path Data="M 0 0 L 4 4 L 8 0 Z"
                                      Fill="White" Stretch="Uniform" Width="12" Height="12"
                                      VerticalAlignment="Center" HorizontalAlignment="Center" />
                                                </Grid>
                                            </ToggleButton>
                                        </Grid>
                                    </Border>
                                    <Popup x:Name="PART_Popup" Placement="Bottom" IsOpen="{TemplateBinding IsDropDownOpen}"
                       AllowsTransparency="True" PopupAnimation="Slide" StaysOpen="False">
                                        <Border Background="White" CornerRadius="8" BorderBrush="#0078D7" BorderThickness="1"
                            MinWidth="{Binding ActualWidth, ElementName=border}">
                                            <ScrollViewer>
                                                <ItemsPresenter />
                                            </ScrollViewer>
                                        </Border>
                                    </Popup>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsDropDownOpen" Value="True">
                                        <Setter TargetName="border" Property="BorderBrush" Value="#00BCD4" />
                                    </Trigger>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#6E6E6E" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </ComboBox.Template>

                        <ComboBoxItem Content="Select Version" IsSelected="True" IsEnabled="False" Foreground="Black" />
                        <ComboBoxItem Content="Global" Foreground="Black" />
                        <ComboBoxItem Content="Korea" Foreground="Black" />
                        <ComboBoxItem Content="Taiwan" Foreground="Black" />
                        <ComboBoxItem Content="Vietnam" Foreground="Black" />
                    </ComboBox>

                    <Button Grid.Row="0" Grid.Column="2" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="ينسخ الموارد من الشير 1 إلى اللعبة ويجب يكون المحاكي شغال" Click="PaksIn_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="ArrowRightBoldCircle" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Paks In" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="1" Grid.Column="0" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="يقوم بفتح المحاكي" Click="OpenGameloop_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="OpenInApp" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Open Gameloop" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="1" Grid.Column="1" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="يقوم بإغلاق المحاكي" Click="KillGameloop_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="Power" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Kill Gameloop" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="1" Grid.Column="2" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="اصلاح مشكلة 98" Click="FixGameloop_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="Wrench" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Fix 98% Gameloop" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="2" Grid.Column="0" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="تعديل دقة ببجي الكورية إلى 1080 ويجب يكون والمحاكي مغلق" Click="P1080P_Korea_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="Monitor" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="1080p Pubg Korea" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="2" Grid.Column="1" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="تشغيل جميع أزرار تحكم الكورية" Click="Key_Keymap_Kr_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="Keyboard" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Key Keymap Kr" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="2" Grid.Column="2" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="إرجاع التحكم الأصلي الخاص بالمحاكي" Click="Key_Keymap_Gl_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="SettingsBackupRestore" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Key Keymap Gl" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>

                    <Button Grid.Row="3" Grid.Column="0" Width="180" Height="50" Margin="10"
        Background="#31384E" Foreground="White" FontSize="14" FontWeight="Bold"
        ToolTip="تنظيف اللعبة ويجب يكون المحاكي شغال" Click="CleanPubg_Click"
        Style="{StaticResource MaterialDesignRaisedButton}">
                        <Button.Template>
                            <ControlTemplate TargetType="Button">
                                <Grid>
                                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="8" 
                        BorderThickness="2" BorderBrush="#0078D7">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="8,0">
                                            <materialDesign:PackIcon Kind="Tools" Width="25" Height="25" Margin="0,0,8,0" />
                                            <TextBlock Text="Clean Pubg Mobile" VerticalAlignment="Center" FontWeight="Bold" />
                                        </StackPanel>
                                    </Border>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#1C2639" />
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter TargetName="border" Property="Background" Value="#31384E" />
                                    </Trigger>
                                    <Trigger Property="IsEnabled" Value="False">
                                        <Setter TargetName="border" Property="Background" Value="#0a0b0f" />
                                        <Setter TargetName="border" Property="BorderBrush" Value="#0078D7" />
                                        <Setter Property="Foreground" Value="#6E6E6E" />
